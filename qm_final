void player()
{
	char name[20],choice;
	printf("\n\t\t\tENTER YOUR NAME: ");
	scanf("%s",name);
	fflush(stdin);
	printf("\n\t\t\t WELC0ME TO ROUND 1 OF THE QUIZ\n\n");
	printf("\n\t\t\tENTER 'S' FOR SCIENCE\n");
	printf("\n\t\t\tENTER 'T' FOR SPORTS\n");
	printf("\n\t\t\tENTER 'M' FOR MOVIES\n");
	printf("\n\t\t\tENTER 'C' FOR COMPUTERS TECHNOLOGIES\n");
	printf("\n\t\t\tENTER YOUR CHOICE: ");
	scanf("%c",&choice);
	switch(choice)
	{
		case'S':
		case's':str1();
		break;
		case'T':
		case't':spr1();
		break;
		case'M':
		case'm':mr1();
		break;
		case'C':
		case'c':cr1();
		default:printf("WRONG INPUT");
	}
}
int main()
{
	printf("\n\t\t\tWELCOME TO QUIZ\n");
	printf("\n\t\t\t-------------------\n");
	printf("\n\t\t\tHERE ARE SOME RULES OF THE QUIZ:\n\n");
	printf("\n\t\t\tRULE 1: YOU NEED TO SELECT THE TOPIC ");
	printf("FOR WHICH YOU WANT TO PLAY THE QUIZ\n");
	printf("\n\t\t\tRULE 2:THERE WILL BE THREE ROUNDS IN THE QUIZ.\n");
	printf("\n\t\t\tRULE 3:YOU WILL BE ASKED 4 QUESTIONS IN 1ST ROUND.\n");
	printf("\n\t\t\tRULE 4:YOU WILL BE ASKED 3 QUESTIONS IN 2ND ROUND.\n");
	printf("\n\t\t\tRULE 5:YOU WILL BE ASKED 5 QUESTIONS IN 3RD ROUND.\n");
	printf("\n\t\t\tRULE 6:IN 1ST ROUND YOU NEED TO SCORE MINIMUM 10 POINTS  THEN YOU WILL BE QUALIFIED FOR 2ND ROUND.\n");
	printf("\n\t\t\tRULE 7:IN 2ND ROUND YOU NEED TO SCORE MINIMUM 15 POINTS THEN YOU WILL BE QUALIFIED FOR 3RD ROUND.\n");
	printf("\n\t\t\tRULE 8:YOU CANNOT LEAVE THE GAME IN BETWEEN \n");
	printf("\n\t\t\tRULE 3:YOU CANNOT CHANGE THE ANSWER ONCE ENTERED.\n\n");
	printf("\n\t\t\t ALL THE BEST\n");
	printf("\t\t\tENTER S TO START THE GAME\n");
	printf("\t\t\tENTER Q TO EXIT THE GAME\n");
	printf("\t\t\tENTER YOUR CHOICE:  ");
	char ch;
	scanf("%c",&ch);
	switch(ch)
	{
		case'S':
		case's':player();
		break;
		case'Q':
		case'q':exit(1);
	}
    return 1;
}
