#define C printf("\t\t\t CORRECT ANSWER ");

void cr3(int pt)
{
    printf("\n\n\n\t\t\tWELCOME TO 3RD ROUND!!!!!\n");
	printf("\n\t\t\tHERE EACH QUESTION CARRIES 15 POINTS\n\n");
    char a1,a2,a3,a4,a5;
	int po=0;
	printf("\n\t\t\tQ1.WHO IS CREDITED WITH INVENTING THE 1ST MECHANICAL COMPUTER??\n");
	printf("\n\t\t\tA. WILLIAM OUGHTRED\t B.KONRAD ZUSE\n");
	printf("\n\t\t\tC. CHARLES BABBAGE \t D.NONE OF THE ABOVE\n");
	A
	F
	scanf("%c",&a1);
	if(a1=='C'||a1=='c')
	{
		C
		pt=pt+15;
		po=po+15;
	}
	else
		printf("\t\tOPPS!!WRONG ANSWER..OPTION 'C' IS THE CORRECT ANSWER\n\n");
	printf("\n\t\t\tQ2.COMPUTER HARD DISK WAS FIRST INTRODUCED IN 1956 BY? \n");
	printf("\n\t\t\tA. DELL \t B.IBM \n");
	printf("\n\t\t\tC. APPLE\t D.MICROSOFT\n");
	A
	F
	scanf("%c",&a2);
	if(a2=='B'||a1=='b')
	{
		C
		pt=pt+15;
		po=po+15;
	}
	else
		printf("\t\tOPPS!!WRONG ANSWER..OPTION 'B' IS THE CORRECT ANSWER\n\n");
	printf("\n\t\t\tQ3.IN COMPUTER WORLD,TROJAN REFERS RO?\n");
	printf("\n\t\t\tA. VIRUS  \t B. WORM\n");
	printf("\n\t\t\tC. MALWARE\t D. SPYWARE\n");
	A
	F
	scanf("%c",&a3);
	if(a3=='C'||a3=='c')
	{
        C
		pt=pt+15;
		po=po+15;
	}
	else
		printf("\t\tOPPS!!WRONG ANSWER..OPTION 'C' IS THE CORRECT ANSWER\n\n");
	printf("\n\t\t\tQ4.WHICH PROTOCOL IS USED TO SEND E-MAIL?\n");
	printf("\n\t\t\tA. POP3\t B.SMTP\n");
	printf("\n\t\t\tC. HTTP\t D.SSH\n");
